#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/bin/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/npm-packlist@5.1.3/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/bin/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/npm-packlist@5.1.3/node_modules/npm-packlist/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/npm-packlist@5.1.3/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/index.js" "$@"
else
  exec node  "$basedir/../../bin/index.js" "$@"
fi
