#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/@sveltejs+package@2.2.6_svelte@5.0.0-next.60_typescript@5.3.3/node_modules/@sveltejs/package/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/@sveltejs+package@2.2.6_svelte@5.0.0-next.60_typescript@5.3.3/node_modules/@sveltejs/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/@sveltejs+package@2.2.6_svelte@5.0.0-next.60_typescript@5.3.3/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/@sveltejs+package@2.2.6_svelte@5.0.0-next.60_typescript@5.3.3/node_modules/@sveltejs/package/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/@sveltejs+package@2.2.6_svelte@5.0.0-next.60_typescript@5.3.3/node_modules/@sveltejs/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/@sveltejs+package@2.2.6_svelte@5.0.0-next.60_typescript@5.3.3/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/package/svelte-package.js" "$@"
else
  exec node  "$basedir/../@sveltejs/package/svelte-package.js" "$@"
fi
