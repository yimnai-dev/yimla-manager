#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/svelte-check@3.6.4_postcss@8.4.35_svelte@5.0.0-next.60/node_modules/svelte-check/bin/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/svelte-check@3.6.4_postcss@8.4.35_svelte@5.0.0-next.60/node_modules/svelte-check/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/svelte-check@3.6.4_postcss@8.4.35_svelte@5.0.0-next.60/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/svelte-check@3.6.4_postcss@8.4.35_svelte@5.0.0-next.60/node_modules/svelte-check/bin/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/svelte-check@3.6.4_postcss@8.4.35_svelte@5.0.0-next.60/node_modules/svelte-check/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/svelte-check@3.6.4_postcss@8.4.35_svelte@5.0.0-next.60/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
