#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/publint@0.1.16/node_modules/publint/lib/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/publint@0.1.16/node_modules/publint/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/publint@0.1.16/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/publint@0.1.16/node_modules/publint/lib/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/publint@0.1.16/node_modules/publint/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/publint@0.1.16/node_modules:/home/yimnai_dev/PROJECTS/personal_projects/yimla-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
